<div id="wiki-wrapper" class="results">
<div id="head">
  <h1>Wyniki wyszukiwania <strong>{{query}}</strong></h1>
</div>
<div id="results">

{{#has_results}}
  <ul>
    {{#results}}
    <li>
      <a href="{{base_url}}/{{name}}">{{name}}</a>
      <span class="count">({{count}} trafień)</span>
    </li>
    {{/results}}
  </ul>
{{/has_results}}

{{#no_results}}
  <p id="no-results">
    There are no results for your search <strong>{{query}}</strong>.
  </p>
{{/no_results}}

</div>
<div id="footer">
  <ul class="actions">
    <li class="minibutton"><a href="#">Do góry</a></li>
  </ul>
</div>
</div>

